<html>
	<head>
		<title>ArtMap</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
		<script src="artmap.js"></script>
	</head>
	<body>
		<div class="container">
			</br>
			<div class="row">
				<!-- Map section -->
				<div class="col-md-3 col-md-offset-2">
					<div class="well">
						<img class="img-responsive" src=<%= imgurl %>>
					</div>
				</div>

				<!-- Filter and info section -->
				<div class="col-md-5">
					<div class=well>
						<h3 style="margin-top:0"><%= title %></h3>
						<p>General Category: <%= gencat %></p>
						<p>Specific Category: <%= speccat %></p>
						<p>Award: <%= award %></p>
						<p>Entered in the <%= year %> <%= city %> Games</p>

						<h4>Artist Info:</h4>
						<p><a href="/competitor?id=<%=pid%>"><%= competitor %></a></p>
						<p>Team: <%= team %></p>
						<% if(submissionCity!=""){ %>
							<p>Submitted from: <%= submissionCity %></p>
							<% } %>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>
